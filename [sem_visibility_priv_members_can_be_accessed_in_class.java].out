.CODE
PUSH simple_heap_init
CALL
PUSH main@Main
CALL
HALT

simple_heap_init: RET 0 ; Retorna inmediatamente

simple_malloc:
LOADFP ; Inicialización unidad
LOADSP
STOREFP ; Finaliza inicialización del RA
LOADHL ; hl
DUP ; hl
PUSH 1 ; 1
ADD ; hl + 1
STORE 4 ; Guarda el resultado (un puntero a la primer celda de la región de memoria)
LOAD 3 ; Carga cantidad de celdas a alojar (parámetro)
ADD
STOREHL ; Mueve el heap limit (hl)
STOREFP ; Almacena el tope de la pila en el registro fp
RET 1 ; Retorna eliminando el parámetro




.DATA
VT_A: DW met@A,m@A

.CODE
met@A:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
FMEM 0
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 1
m@A:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
RMEM 1 ; guardo lugar para la variable local x
LOAD 3
LOADREF 1 ; offset atributo
STORE 0
RMEM 1 ; guardo lugar para la variable local y
LOAD 3
RMEM 1 ; reservo lugar para el return
SWAP
DUP
LOADREF 0 ; Cargo la VT
LOADREF 0
CALL
STORE -1
RMEM 1 ; guardo lugar para la variable local z
RMEM 1
PUSH 2; apilo los atributos
PUSH simple_malloc
CALL
DUP
PUSH VT_A; apilo el comienzo de la vt
STOREREF 0
DUP



PUSH lblConstructor@A
CALL
STORE -2
FMEM 3
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 1



lblConstructor@A:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
FMEM 0
FMEM 0
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 1



.DATA
VT_X: DW m@X

.CODE
m@X:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
RMEM 1 ; guardo lugar para la variable local x
RMEM 1
PUSH 2; apilo los atributos
PUSH simple_malloc
CALL
DUP
PUSH VT_X; apilo el comienzo de la vt
STOREREF 0
DUP



PUSH lblConstructor@X
CALL
STORE 0
PUSH 2 ; Apila el entero 2
DUP
LOAD 0
SWAP
STOREREF 1
POP
FMEM 1
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 1



lblConstructor@X:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
FMEM 0
FMEM 0
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 1



