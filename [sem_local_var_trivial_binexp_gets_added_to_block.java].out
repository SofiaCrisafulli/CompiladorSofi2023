m@Main:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
RMEM 1 ; guardo lugar para la variable local add
PUSH 3 ; Apila el entero 3
PUSH 3 ; Apila el entero 3
ADD
STORE 0
RMEM 1 ; guardo lugar para la variable local sub
PUSH 3 ; Apila el entero 3
PUSH 3 ; Apila el entero 3
SUB
STORE -1
RMEM 1 ; guardo lugar para la variable local mul
PUSH 3 ; Apila el entero 3
PUSH 3 ; Apila el entero 3
MUL
STORE -2
RMEM 1 ; guardo lugar para la variable local div
PUSH 3 ; Apila el entero 3
PUSH 3 ; Apila el entero 3
DIV
STORE -3
RMEM 1 ; guardo lugar para la variable local mod
PUSH 3 ; Apila el entero 3
PUSH 3 ; Apila el entero 3
MOD
STORE -4
RMEM 1 ; guardo lugar para la variable local and
PUSH 1 ; apilo true
PUSH 0 ; apilo false
AND
STORE -5
RMEM 1 ; guardo lugar para la variable local or
PUSH 1 ; apilo true
PUSH 0 ; apilo false
OR
STORE -6
RMEM 1 ; guardo lugar para la variable local eq
PUSH 1 ; Apila el entero 1
PUSH 1 ; Apila el entero 1
EQ
STORE -7
RMEM 1 ; guardo lugar para la variable local neq
PUSH 1 ; Apila el entero 1
PUSH 1 ; Apila el entero 1
NE
STORE -8
FMEM 9
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 1



