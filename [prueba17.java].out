.CODE
PUSH simple_heap_init
CALL
PUSH main@Init
CALL
HALT

simple_heap_init: RET 0 ; Retorna inmediatamente

simple_malloc:
LOADFP ; Inicialización unidad
LOADSP
STOREFP ; Finaliza inicialización del RA
LOADHL ; hl
DUP ; hl
PUSH 1 ; 1
ADD ; hl + 1
STORE 4 ; Guarda el resultado (un puntero a la primer celda de la región de memoria)
LOAD 3 ; Carga cantidad de celdas a alojar (parámetro)
ADD
STOREHL ; Mueve el heap limit (hl)
STOREFP ; Almacena el tope de la pila en el registro fp
RET 1 ; Retorna eliminando el parámetro




.DATA
VT_Rec: DW rec$int@Rec

.CODE
rec$int@Rec:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
LOAD 4
PUSH 0 ; Apila el entero 0
LT
BF labelThen4 ; salta afuera del if si la condicion es falsa
FMEM 0
STOREFP ; Almacena el tope de la pila en el registro fp
RET 2



labelElse5: NOP



LOAD 4
PUSH debugPrint$int@Object ; Apila el método 
CALL ; Llama al método en el tope de la pila
LOAD 3
LOAD 4
PUSH 1 ; Apila el entero 1
SUB
DUP
LOADREF 0 ; Cargo la VT
LOADREF 0
CALL
FMEM 0
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 2



lblConstructor@Rec:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
FMEM 0
FMEM 0
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 1



.DATA
VT_Init: NOP

.CODE
main@Init:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
RMEM 1 ; guardo lugar para la variable local r
RMEM 1
PUSH 1; apilo los atributos
PUSH simple_malloc
CALL
DUP
PUSH VT_Rec; apilo el comienzo de la vt
STOREREF 0
DUP



PUSH lblConstructor@Rec
CALL
STORE 0
LOAD 0
PUSH 5 ; Apila el entero 5
SWAP
DUP ; no perder el this
LOADREF 0 ; cargo la vt
LOADREF 0
CALL
FMEM 1
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 0



lblConstructor@Init:
LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
FMEM 0
FMEM 0
STOREFP ;AF Almacena el tope de la pila en el registro fp
RET 1



